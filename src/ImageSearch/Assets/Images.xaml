<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <BitmapImage
        x:Key="AppIcon"
        UriSource="./Images/AppIcon.ico" />

    <BitmapImage
        x:Key="AppIcon_16px"
        DecodePixelHeight="16"
        DecodePixelWidth="16"
        UriSource="./Images/AppIcon.ico" />

    <BitmapImage
        x:Key="DanbooruIcon"
        UriSource="./Images/Danbooru.ico" />

    <BitmapImage
        x:Key="GelbooruIcon"
        UriSource="./Images/Gelbooru.png" />
</ResourceDictionary>